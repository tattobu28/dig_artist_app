<%= render "shared/header" %>
<main class="main">
  <div class="inner">
    <% if artist_signed_in? %>
      <div class="greeting">
        <p> Hello!
        <%= link_to current_artist.name, "/artists/#{current_artist.id}", class: :greeting__link %>
        </p>
      </div>
    <% end %>
    <div class="newitem">
    <p>New Songs</p>
    </div>
    <div class="card__wrapper">
      <%= render partial: "song", collection: @songs %>
    </div>
    <div class="newitem">
    <p>New Artists</p>
    </div>
    <div class="artist__wrapper">
      <%= render partial: "artist", collection: @artists %>
    </div>
    <div class="newitem">
    <p>New MusicVideo</p>
    </div>
    <div class="artist__wrapper">
      <%= render partial: "youtube", collection: @youtubes %>
    </div>
  </div>
</main>
<%= render "shared/footer" %>